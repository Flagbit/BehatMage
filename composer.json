{
    "name": "magetest/magento-behat-extension",
    "description": "Magento Behat extension",
    "require": {
        "behat/behat": "2.4.4",
        "behat/mink-extension": "*",
        "behat/mink-browserkit-driver": "*",
        "behat/mink-goutte-driver": "*"
    },
    "autoload": {
        "psr-0": {
            "MageTest": "src/",
            "": ["vendor/magento/lib","vendor/magento/app"],
            "Mage": "vendor/magento/app/code/core"
        }
    },
    "require-dev": {
        "phpspec/phpspec2": "*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "authors": [
